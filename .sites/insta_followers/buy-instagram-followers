                <script>
                    function choose_plan (plan)
                    {
                        $.post("", {get_plan_block: plan}, function(data) {
                                res = $.parseJSON(data);
                                if (res.result == 'Ok') {
                                    if (res.html) {
                                        $('#block_choose_plan').replaceWith(res.html);
                                        $("#block_choose_plan .timer").each(function () {
                                            start_timer(this);
                                        });
                                    }
                                    if (res.scroll_to_id)
                                        $('html,body').animate({ scrollTop: $('#'+res.scroll_to_id).offset().top - 125}, 500);
                                   
                                } else if (res.result == 'Error') {
                                    alert(res.text);
                                    if (res.reload)
                                        window.location = window.location;
                                }
                        });
                    }
                    
                    function choose_plan_clear ()
                    {
                        $.post("", {get_all_plans_block: 1}, function(data) {
                                res = $.parseJSON(data);
                                if (res.result == 'Ok') {
                                    if (res.html) {
                                        $('#block_choose_plan').replaceWith(res.html);
                                        $("#block_choose_plan .timer").each(function () {
                                            start_timer(this);
                                        });
                                    }
                                } else if (res.result == 'Error') {
                                    alert(res.text);
                                    if (res.reload)
                                        window.location = window.location;
                                }
                        });
                    }
                </script>
    
        
